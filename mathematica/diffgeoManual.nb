490bf5e9e317"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"$Assumptions", "=", " ", 
    RowBox[{"And", "[", 
     RowBox[{
      RowBox[{"t", "\[Element]", "Reals"}], ",", 
      RowBox[{"\[Phi]", "\[Element]", "Reals"}], ",", 
      RowBox[{"\[Theta]", ">", "0"}], ",", 
